{
  "package": {
    "version": "0.1.0",
    "dependencies": {
      "@testing-library/jest-dom": "^5.14.1",
      "@testing-library/react": "^12.0.0",
      "@testing-library/user-event": "^13.2.1",
      "@types/jest": "^27.0.1",
      "@types/node": "^16.7.13",
      "@types/react": "^17.0.20",
      "@types/react-dom": "^17.0.9",
      "react": "^17.0.2",
      "react-dom": "^17.0.2",
      "react-router-dom": "^5.3.0",
      "react-scripts": "4.0.3",
      "typescript": "^4.4.2",
      "web-vitals": "^2.1.0"
    },
    "devDependencies": {
      "@commitlint/cli": "^13.2.1",
      "@commitlint/config-conventional": "^13.2.0",
      "@typescript-eslint/eslint-plugin": "^5.1.0",
      "@typescript-eslint/parser": "^5.1.0",
      "cors": "^2.8.5",
      "eslint": "^7.30.0",
      "eslint-config-airbnb": "^18.2.1",
      "eslint-config-prettier": "^8.3.0",
      "eslint-config-react-app": "^6.0.0",
      "eslint-plugin-flowtype": "^6.1.1",
      "eslint-plugin-import": "^2.25.2",
      "eslint-plugin-jest": "^25.2.2",
      "eslint-plugin-jsx-a11y": "^6.4.1",
      "eslint-plugin-prettier": "^4.0.0",
      "eslint-plugin-react": "^7.26.1",
      "eslint-plugin-react-hooks": "^4.2.0",
      "eslint-plugin-react-redux": "^3.3.2",
      "eslint-plugin-test-selectors": "^1.3.2",
      "express": "^4.17.1",
      "husky": "^7.0.0",
      "prettier": "^2.4.1"
    },
    "scripts": {
      "start": "react-scripts start",
      "start:safari": "BROWSER='safari' yarn start",
      "start:firefox": "BROWSER='firefox' yarn start",
      "server": "node --experimental-modules ./mockServer.js",
      "build": "react-scripts build",
      "test": "react-scripts test",
      "eject": "react-scripts eject",
      "check-types": "tsc --noemit",
      "audit": "yarn run lint && yarn test a --watchAll=false",
      "lint": "yarn run check-types && eslint --ext .ts --ext .tsx --ext .js .",
      "lint:err": "yarn run lint -- --quiet",
      "lint:fix": "yarn run prettier && npm run lint -- --fix --fix-type layout,suggestion,problem",
      "prettier": "prettier '**/*.+(js|ts|tsx)' --write",
      "prepare": "husky install"
    },
    "eslintConfig": {
      "extends": "react-app"
    },
    "browserslist": {
      "production": [
        ">0.2%",
        "not dead",
        "not op_mini all"
      ],
      "development": [
        "last 1 chrome version",
        "last 1 firefox version",
        "last 1 safari version"
      ]
    },
    "jest": {
      "collectCoverageFrom": [
        "src/**/*.ts",
        "src/**/*.tsx",
        "!src/**/*.d.ts"
      ],
      "moduleNameMapper": {
        "\\.(css|less|scss|sass)$": "identity-obj-proxy"
      }
    }
  }
}
