{
  "package": {
    "version": "0.1.0",
    "dependencies": {
      "@testing-library/jest-dom": "^5.16.1",
      "@testing-library/react": "^12.1.2",
      "@testing-library/user-event": "^13.5.0",
      "@types/jest": "^27.0.3",
      "@types/node": "^16.11.13",
      "@types/react": "^17.0.37",
      "@types/react-dom": "^17.0.11",
      "react": "^17.0.2",
      "react-dom": "^17.0.2",
      "react-router-dom": "^6.1.1",
      "react-scripts": "^5.0.0",
      "typescript": "^4.5.4",
      "web-vitals": "^2.1.2",
      "standard-version": "^9.3.2"
    },
    "devDependencies": {
      "@commitlint/cli": "^15.0.0",
      "@commitlint/config-conventional": "^15.0.0",
      "@typescript-eslint/eslint-plugin": "^5.7.0",
      "@typescript-eslint/parser": "^5.7.0",
      "autoprefixer": "^10.4.0",
      "eslint": "^8.4.1",
      "eslint-config-airbnb": "^19.0.2",
      "eslint-config-prettier": "^8.3.0",
      "eslint-config-react-app": "^7.0.0",
      "eslint-plugin-flowtype": "^8.0.3",
      "eslint-plugin-import": "^2.25.3",
      "eslint-plugin-jest": "^25.3.0",
      "eslint-plugin-jsx-a11y": "^6.5.1",
      "eslint-plugin-prettier": "^4.0.0",
      "eslint-plugin-react": "^7.27.1",
      "eslint-plugin-react-hooks": "^4.3.0",
      "eslint-plugin-react-redux": "^3.3.2",
      "eslint-plugin-test-selectors": "^1.3.2",
      "husky": "^7.0.4",
      "postcss": "^8.4.5",
      "prettier": "^2.5.1",
      "tailwindcss": "^3.0.2"
    },
    "scripts": {
      "start": "react-scripts start",
      "start:safari": "BROWSER='safari' yarn start",
      "start:firefox": "BROWSER='firefox' yarn start",
      "server": "node --experimental-modules --experimental-json-modules ./mockServer.mjs",
      "build": "react-scripts build",
      "test": "react-scripts test",
      "eject": "react-scripts eject",
      "check-types": "tsc --noemit",
      "audit": "yarn run lint && yarn test a --watchAll=false",
      "lint": "yarn run check-types && eslint --ext .ts --ext .tsx --ext .js .",
      "lint:err": "yarn run lint -- --quiet",
      "lint:fix": "yarn run prettier && npm run lint -- --fix --fix-type layout,suggestion,problem",
      "prettier": "prettier '**/*.+(js|ts|tsx)' --write",
      "prepare": "husky install"
    },
    "eslintConfig": {
      "extends": "react-app"
    },
    "browserslist": {
      "production": [
        ">0.2%",
        "not dead",
        "not op_mini all"
      ],
      "development": [
        "last 1 chrome version",
        "last 1 firefox version",
        "last 1 safari version"
      ]
    },
    "jest": {
      "collectCoverageFrom": [
        "src/**/*.ts",
        "src/**/*.tsx",
        "!src/**/*.d.ts"
      ],
      "moduleNameMapper": {
        "\\.(css|less|scss|sass)$": "identity-obj-proxy"
      }
    }
  }
}
